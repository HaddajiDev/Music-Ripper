{
  "manifest_version": 2,
  "name": "Music Ripper",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "downloads",
    "*://*.youtube.com/*",
    "https://music-ripper.onrender.com/*",
    "storage",
    "cookies"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  }
}